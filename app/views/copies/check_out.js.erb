<% if @check_out.model.new_record? %>
  $('.alert-message').remove();
  $('#mainModal .modal-body').prepend("<%= j render :partial => 'shared/flash', :locals => { :flash => flash }  %>");
  $('#check-out').replaceWith("<%= j render :partial => 'check_out', :locals => { :order => @order } %>");
  $(document).trigger('CheckOut.failure')
<% else %>
  $('tr#book-<%= @copy.book.id %>').replaceWith('<%= j render :partial => 'books/book', :locals => { :book => @copy.book } %>');
  $('tr#book-<%= @copy.book.id %>').effect("highlight", {}, 5000)
  $(document).trigger('CheckOut.success')
<% end %>

